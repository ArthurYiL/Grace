<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <version>$version$</version>
    <authors>Ian Johnson</authors>
    <owners>Ian Johnson</owners>
    <licenseUrl>https://github.com/ipjohnson/Grace/blob/master/License.md</licenseUrl>
    <projectUrl>https://github.com/ipjohnson/Grace</projectUrl>
    <id>Grace.log4net</id>
    <title>Grace.log4net</title>
    <summary>Grace.log4net provides a logging service using log4net and Grace</summary>
    <description>Grace.log4net provides integration classes between log4net and Grace (Dependency Injection Container)</description>
    <releaseNotes>This bug fix release, included is a fix for importing properties on classes with static properties. Also included is a fix that allows the nuget package to install into portable class libraries.</releaseNotes>
    <copyright>Copyright 2014 Ian Johnson</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <tags>IoC Dependency Injection Container MVC</tags>
    <dependencies>
      <dependency id="Grace" version="$version$"/>
      <dependency id="log4net" version="2.0.3"/>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\net45\Grace.log4net.dll" target="lib\net45\Grace.log4net.dll" />
    <file src="lib\net45\Grace.log4net.pdb" target="lib\net45\Grace.log4net.pdb" />
    <file src="lib\net45\Grace.log4net.xml" target="lib\net45\Grace.log4net.xml" />
    
    <file src="..\Source\Grace.log4net\**\*.cs" target="src"/>
  </files>
</package>
