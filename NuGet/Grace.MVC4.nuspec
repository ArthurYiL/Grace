    <copyright>Copyright 2014 Ian Johnson</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <tags>IoC Dependency Injection Container MVC 4</tags>
    <dependencies>
      <dependency id="Grace" version="$version$"/>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\net45\Grace.MVC4.dll" target="lib\net45\Grace.MVC4.dll" />
    <file src="lib\net45\Grace.MVC4.pdb" target="lib\net45\Grace.MVC4.pdb" />
    <file src="lib\net45\Grace.MVC4.xml" target="lib\net45\Grace.MVC4.xml" />
    
    <file src="..\Source\Grace.MVC5\**\*.cs" target="src"/>
  </files>
</package>
